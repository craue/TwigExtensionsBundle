{
	"name": "craue/twigextensions-bundle",
	"type": "symfony-bundle",
	"description": "Useful Twig extensions for your Symfony project.",
	"keywords": ["twig", "extensions", "symfony"],
	"homepage": "https://github.com/craue/TwigExtensionsBundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Raue",
			"email": "christian.raue@gmail.com"
		},
		{
			"name": "Symfony Community",
			"homepage": "https://github.com/craue/TwigExtensionsBundle/contributors"
		}
	],
	"require": {
		"php": "^7.3|^8",
		"symfony/config": "~3.4|~4.4|~5.3",
		"symfony/dependency-injection": "~3.4|~4.4|~5.3",
		"symfony/twig-bundle": "~3.4|~4.4|~5.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"symfony/browser-kit": "~3.4|~4.4|~5.3",
		"symfony/form": "~3.4|~4.4|~5.3",
		"symfony/framework-bundle": "~3.4|~4.4|~5.3",
		"symfony/phpunit-bridge": "~5.3",
		"symfony/translation": "~3.4|~4.4|~5.3",
		"symfony/yaml": "~3.4|~4.4|~5.3"
	},
	"minimum-stability": "stable",
	"autoload": {
		"psr-4": {
			"Craue\\TwigExtensionsBundle\\": ""
		},
		"exclude-from-classmap": [
			"/Tests/"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.9.x-dev"
		},
		"symfony": {
			"require": "~3.4.23|~4.4|~5.3"
		}
	},
	"config": {
		"sort-packages": true
	}
}
